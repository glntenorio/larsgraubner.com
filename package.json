{
  "name": "larsgraubner.com",
  "description": "Personal blog",
  "version": "1.0.0",
  "author": "Lars Graubner <mail@larsgraubner.de>",
  "dependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "date-fns": "1.29.0",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-remark-copy-linked-files": "next",
    "gatsby-remark-images": "next",
    "gatsby-remark-prismjs": "next",
    "gatsby-remark-responsive-iframe": "next",
    "gatsby-remark-smartypants": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "idx": "2.4.0",
    "is-absolute-url": "2.1.0",
    "polished": "1.9.3",
    "prism-themes": "1.0.1",
    "prismjs": "1.15.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-helmet": "5.2.0",
    "styled-components": "3.4.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.79.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "mri": "1.1.1",
    "prettier": "1.14.2",
    "slugify": "1.3.1"
  },
  "homepage": "https://larsgraubner.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgraubner/larsgraubner.com.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js --ignore-pattern public .",
    "flow": "flow",
    "format": "prettier --write 'src/**/*.js'",
    "build": "gatsby build",
    "create-post": "node create-post.js",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {},
    "globals": {
      "graphql": false,
      "__PATH_PREFIX__": false,
      "__PREFIX_PATHS__": false
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
